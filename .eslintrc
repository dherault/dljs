{
  "extends": "airbnb-base",
  "rules": {
    "brace-style": 0,
    "guard-for-in": 0,
    "consistent-return": 0,
    "max-len": 0,
    "no-confusing-arrow": 0,
    "no-console": 0,
    "no-constant-condition": 0,
    "no-multi-spaces": 1,
    "no-new": 0,
    "no-nested-ternary": 0,
    "no-restricted-syntax": 0,
    "no-return-assign": 0,
    "no-self-compare": 0,
    "no-shadow": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "no-use-before-define": 0,
    "one-var": ["error", { "uninitialized": "always", "initialized": "never" }],
    "one-var-declaration-per-line": 0,
    "padded-blocks": 0,
    "semi": 1,
    "space-in-parens": 1,
  }
}
