{
  "extends": "airbnb-base",
  "rules": {
    "no-restricted-syntax": 0,
    "max-len": 0,
    "padded-blocks": 0,
    "guard-for-in": 0,
    "no-console": 0,
    "no-confusing-arrow": 0,
    "no-new": 0,
    "no-return-assign": 0,
    "no-use-before-define": 0,
    "one-var": ["error", { "uninitialized": "always", "initialized": "never" }],
    "one-var-declaration-per-line": 0,
  }
}
